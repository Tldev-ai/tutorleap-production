    { key: 'regionalAdvantages', label: 'Regional Advantages', icon: 'üó∫Ô∏è' }
  ];

  const handleConvert = () => {
    if (fromBoard && toBoard && grade && subject) {
      setShowComparison(true);
    }
  };

  const resetForm = () => {
    setFromBoard('');
    setToBoard('');
    setGrade('');
    setSubject('');
    setShowComparison(false);
    setSelectedCategory('standardTerminology');
  };

