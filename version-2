    { key: 'supplementaryExams', label: 'Supplementary Exams', icon: 'üîÑ' },
    { key: 'academicStructure', label: 'Academic Structure', icon: 'üèõÔ∏è' },
    { key: 'examPattern', label: 'Exam Pattern', icon: '‚úçÔ∏è' },
    { key: 'universityAdmission', label: 'University Admission', icon: 'üéì' },
