    { key: 'careerPathways', label: 'Career Pathways', icon: 'üíº' },
    { key: 'costAnalysis', label: 'Cost Analysis', icon: 'üí∞' },
    { key: 'timeInvestment', label: 'Time Investment', icon: '‚è∞' },
